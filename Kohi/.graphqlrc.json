{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
        "namespace": "KohiClient.GraphQL",
        "name": "KohiClient",
        "url": "http://localhost:5000/graphql",
        "records": {
            "inputs": false,
            "entities": false
        },
        "transportProfiles": [
            {
                "default": "Http",
                "subscription": "WebSocket"
            }
        ]
    }
  }
}